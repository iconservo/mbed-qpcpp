{
  "target_overrides": {
    "*": {
      "target.uart_hwfc": 0,
      "platform.stdio-baud-rate": 115200,
      "platform.default-serial-baud-rate": 115200,
      "nordic-ble.uuid_table_max_entries": 16,
      "target.heap_size": 8192
    }
  },
  "macros": [
    "MBED_HEAP_STATS_ENABLED=1",
    "MBED_STACK_STATS_ENABLED=1",
    "MBED_MEM_TRACING_ENABLED=0",     
    "MEM_STATS_ENABLED=1",
    "MEM_STATS_TIMEOUT=5000",
    "QF_PORT_DEBUG=1"
  ],
  "config": {
    "main-stack-size": {
      "value": 4096
    },
    "thread-stack-size": {
      "value": 2048
    }
  }    
}
